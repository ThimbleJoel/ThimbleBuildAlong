const int ledSocket = 6;

void setup() {
  pinMode(ledSocket, OUTPUT);
}

void loop() {
  digitalWrite(ledSocket, HIGH);
  delay(200);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(200);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(200);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(600);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(600);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(600);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(200);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(200);
  digitalWrite(ledSocket, LOW);
  delay(200);
  digitalWrite(ledSocket, HIGH);
  delay(200);
  digitalWrite(ledSocket, LOW);
  delay(1400);
}

